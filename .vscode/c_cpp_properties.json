{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/local/include/**",
                "/usr/include/**",
                "/usr/lib/x86_64-linux-gnu/glib-2.0/include"
            ],
            "defines": [
                "FUUGB_DEBUG",
                "FUUGB_SYSTEM_LINUX",
                "_FILE_OFFSET_BITS=64"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}
