    "version": "0.2.0",
    "configurations": [
        {
            "name": "Tetris",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/FuuGBemu",
            "args": [
                "--skip-boot-rom",
                "testrom/Tetris.gb"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerArgs": "-exec -enable-pretty-printing",
            "setupCommands": [],
            "miDebuggerPath": "/usr/bin/gdb"
        },
